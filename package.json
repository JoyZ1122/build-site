{
  "name": "quail-h5",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev-client": "vue-cli-service serve --mode dev",
    "dev-server": "nodemon server/app.js",
    "dev": "npm run dev-client & npm run dev-server",
    "build-client": "vue-cli-service build",
    "build-test": "rm -rf server/public/static && rm -rf server/public/index.html && vue-cli-service build --mode test && cp -r dist/. server/public/",
    "client-test": "vue-cli-service build --mode test",
    "prd": "pm2 start server/bin/www",
    "server": "pm2 start server/app.js --watch",
    "lib:h5-swiper": "vue-cli-service build --target lib --name page-engine --dest server/public/engine_libs/h5-swiper engine-template/engine-h5-swiper/index.js",
    "lib:h5-long": "rm -rf server/public/engine_libs && vue-cli-service build --target lib --name page-engine --dest server/public/engine_libs/h5-long engine-template/engine-h5-long/index.js",
    "client-long": " vue-cli-service build --target lib --name page-engine --dest server/public/engine_libs/h5-long engine-template/engine-h5-long/index.js",
    "build": "npm run lib:h5-long && npm run build-test"
  },
  "dependencies": {
    "ali-oss": "^6.1.1",
    "animate.css": "^3.7.2",
    "axios": "^0.19.0",
    "babel-polyfill": "^6.26.0",
    "core-js": "^2.6.5",
    "crypto-js": "^3.1.9-1",
    "dom-to-image": "^2.6.0",
    "element-ui": "^2.12.0",
    "html2canvas": "^1.0.0-rc.5",
    "js-md5": "^0.7.3",
    "js-sha1": "^0.6.0",
    "koa-body": "^4.1.1",
    "koa-ejs": "^4.2.0",
    "lodash": "^4.17.15",
    "mysql2": "^2.1.0",
    "nprogress": "^0.2.0",
    "pm2": "^4.4.1",
    "qrcode.vue": "^1.6.3",
    "sequelize": "^6.3.4",
    "vant": "^2.10.2",
    "vue": "^2.6.10",
    "vue-clipboard2": "^0.3.1",
    "vue-cropper": "^0.4.9",
    "vue-router": "^3.0.3",
    "vue-swiper-component": "^2.1.3",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.11.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^3.11.0",
    "art-template": "^4.13.2",
    "babel-eslint": "^10.0.1",
    "babel-plugin-component": "^1.1.1",
    "babel-plugin-import": "^1.12.2",
    "babel-preset-env": "^1.7.0",
    "co": "^4.6.0",
    "compression-webpack-plugin": "^3.0.0",
    "debug": "^4.1.1",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "html2canvas-proxy": "^1.0.1",
    "jsonwebtoken": "^8.5.1",
    "koa": "^2.10.0",
    "koa-art-template": "^1.1.1",
    "koa-bodyparser": "^4.2.1",
    "koa-convert": "^1.2.0",
    "koa-json": "^2.0.2",
    "koa-jwt": "^3.6.0",
    "koa-logger": "^3.2.1",
    "koa-onerror": "^4.1.0",
    "koa-router": "^7.4.0",
    "koa-static": "^5.0.0",
    "koa-views": "^6.2.1",
